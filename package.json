    "jsdom": "^15.2.1"
  },
  "devDependencies": {
    "request": "^2.88.2",
    "unirest": "^0.6.0",
    "less": "^3.12.2",
    "express": "^4.17.1",
    "express-static": "^1.2.6",
    "@babel/core": "^7.11.4",
    "@babel/preset-env": "^7.11.0",
    "babelify": "^10.0.0",
    "browserify": "^16.5.2",
